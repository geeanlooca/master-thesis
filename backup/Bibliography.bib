isbn = {2514599342},
issn = {0036-8075},
journal = {Science. November},
number = {2545035},
pages = {1178--1181},
pmid = {1957169},
title = {{Optical Coherence Tomography HHS Public Access}},
volume = {22},
year = {1991}
}
