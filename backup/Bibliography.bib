@article{Tearney96,
author = {G. J. Tearney and S. A. Boppart and B. E. Bouma and M. E. Brezinski and N. J. Weissman and J. F. Southern and J. G. Fujimoto},
journal = {Opt. Lett.},
keywords = {},
