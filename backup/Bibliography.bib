isbn = {09570233},
issn = {13616501},
journal = {Measurement Science and Technology},
keywords = {calibration,modulation transfer function,optical coherence tomography,phantom,point-spread function,resolution},
number = {6},
title = {{Estimating the resolution of a commercial optical coherence tomography system with limited spatial sampling}},
volume = {22},
year = {2011}
}
