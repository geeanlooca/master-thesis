@article{Woolliams2011,
abstract = {Optical coherence tomography (OCT) is becoming increasingly widespread$\backslash$nas an experimental tool for clinical investigation, facilitated by$\backslash$nthe development of commercial instruments. In situ performance evaluation$\backslash$nof such 'black box' systems presents a challenge, where the instrument$\backslash$nhardware and software can limit access to important configuration$\backslash$nparameters and raw data. Two key performance metrics for imaging$\backslash$nsystems are the point-spread function (PSF) and the associated modulation$\backslash$ntransfer function (MTF). However, previously described experimental$\backslash$nmeasurement techniques assume user-variable spatial sampling and$\backslash$nmay not be appropriate for the characterization of deployed commercial$\backslash$ninstruments. Characterization methods developed for other modalities$\backslash$ndo not address this issue and rely upon experimental accuracy. Therefore,$\backslash$nin this paper we propose a method to characterize the PSF of a commercial$\backslash$nOCT microscope that uses OCT images of three-dimensional PSF phantoms$\backslash$nto produce an oversampled estimate of the system PSF by combining$\backslash$nspatially coincident measurements. This method does not rely upon$\backslash$nany strong a priori assumption of the PSF morphology, requires no$\backslash$nmodification to the system sampling configuration or additional experimental$\backslash$nprocedure. We use our results to determine the PSF and MTF across$\backslash$nthe B-scan image plane of a commercial OCT system.},
author = {Woolliams, Peter D. and Tomlins, Peter H.},
doi = {10.1088/0957-0233/22/6/065502},
